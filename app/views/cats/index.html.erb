<h1>Here are the Cats!</h1>


<ul>
  <% @cats.each do |cat| %>
    <li>
      <a href="<%= cat_url(cat) %>"><%= cat.name %></a>

      <form action="<%= cat_url(cat) %>" method="POST">
        <input type="hidden" name="_method" value="DELETE">
        <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
        <input type="submit" value="Delete Cat">



      </form>
    </li>
  <% end %>
<ul>
<br>

<a href=" <%= new_cat_url %>">Create a cat</a>
<br>
<a href="/cat_rental_requests/new">Submit a request for a cat</a>
